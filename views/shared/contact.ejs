<%- include('./includes/head.ejs') %>

<body class="theme3">
    <%- include('./includes/loader.ejs') %>

    <%- include('./includes/navbar.ejs') %>

    <div class="breadcumb-wrapper" data-bg-src="assets/img/bg/breadcumb-bg.jpg">
        <div class="container">
            <div class="breadcumb-content">
                <h1 class="breadcumb-title">Контакты</h1>
                <ul class="breadcumb-menu">
                    <li><a href="/">Главная страница</a></li>
                    <li>Контакты</li>
                </ul>
            </div>
        </div>
    </div>
    <section class="space">
        <div class="container">
            <div class="about-card">
                <div class="about-card_img"><img class="w-100" src="<% if (profile_meta.image_url) { %> /uploads/img/<%= profile_meta.image_url %> <% } else { %> ./assets/img/team/team_1_2.jpg <% } %>" alt="team image">
                </div>
                <div class="about-card_box">
                    <div class="about-card_top">
                        <div class="media-body">
                            <h2 class="about-card_title"><%= profile_meta.first_name %> <%= profile_meta.last_name %></h2>
                            <p class="about-card_desig"><%= profile_meta.job %></p>
                        </div>
                    </div>
                    <p class="about-card_text"><%= profile_meta.about_me %></p>
                    <div class="team-info-wrap">
                        <div class="contact-feature">
                            <div class="icon-btn"><i class="fa-solid fa-user"></i></div>
                            <div class="media-body">
                                <p class="contact-feature_label">Опыт работы</p><span class="contact-feature_link">Более <%= profile_meta.experience_years %> лет</span>
                            </div>
                        </div>
                        <div class="contact-feature">
                            <div class="icon-btn"><i class="fa-solid fa-phone"></i></div>
                            <div class="media-body">
                                <p class="contact-feature_label">Телефонный номер</p><a href="tel:+998 <%= profile_meta.phone %>"
                                    class="contact-feature_link">+998 <%= profile_meta.phone %></a>
                            </div>
                        </div>
                        <div class="contact-feature">
                            <div class="icon-btn"><i class="fa-solid fa-envelope"></i></div>
                            <div class="media-body">
                                <p class="contact-feature_label">Электронная почта</p><a href="mailto:<%= profile_meta.email %>"
                                    class="contact-feature_link"><%= profile_meta.email %></a>
                            </div>
                        </div>
                        <div class="contact-feature">
                            <div class="icon-btn"><i class="fa-solid fa-calendar-days"></i></div>
                            <div class="media-body">
                                <p class="contact-feature_label">Телеграм</p>
                                <span class="contact-feature_link">
                                    <a class="contact-feature_link" href="https://t.me/<%= profile_meta.telegram %>">
                                        <% if (profile_meta.telegram) { %> <%= profile_meta.first_name %> <%= profile_meta.last_name %> <% } %> 
                                    </a>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    
    <%- include('./includes/footer.ejs') %>
    
    <%- include('./includes/end.ejs') %>

</body>
